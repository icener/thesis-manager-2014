<html>
	
	<head>
		<title> Edit Student </title>
		<meta name="viewport" content="width=device-width, initial-scale=1.0"/> 
		<link href="/css/bootstrap.css" rel="stylesheet"/>	
		<link rel="stylesheet" href="/css/styles.css"/>	
	</head>



<body background="/images/a3pink.jpg">


{% if user_name %}
<!-- Header -->
	<div id="header" class="navbar navbar-inverse navbar-static-top " >
	
		<div id="header_container" class="container">
			<a class = "navbar-brand"><h3 style="font-style: Calibri; color:#E6E6E6;">Module 3<h3></a>
			
			<div >
				<button type="button"  class="btn btn-custom navbar-btn pull-right">
				<a color= #FE8196 href="{{ url|safe }}"> {{ url_linktext }}</a>
				</button>
			</div>

			<div>
          &nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp<ul class="nav nav-pills">
  				<li class="#"><a href="/">Guestbook</a></li>
  				<li class="dropdown">
  					<a href="#" class="dropdown-toggle" data-toggle="dropdown"> Profiles <span class="caret"></span></a>
					<ul class="dropdown-menu">
					   	<li><a href="/module-1/1"> Profile One</a></li>
                    	<li><a href="/module-1/2"> Profile Two</a></li>
  					</ul>  			
  				</li>
  				<li><a href="/student/list">Students</a></li>
  				<li><a href="/thesis/list">Thesis</a></li>
  				<li><a href="/adviser/list">Advisers</a></li>
			</ul>
			</div>


		</div>	

<!-- Header End -->

	</div>

	
	
	<div class="container">
		<div class="row">
			<!-- Side Menu-->
			<div class="col-md-3">
				<br/><br/>
				<a class="btn" id="studbtn" href="/student/new"><img src="/images/pipol.png" width=25px height=25px align="left"> &nbsp&nbsp&nbsp Add new Student</a>
				<br><br>
				<a class="btn" id="studbtn" href="/student/list"><img src="/images/studs.png" width=25px height=25px align="left"> &nbsp&nbsp&nbsp View all Students </a>
			</div>
			<br/>


			<div class="col-md-6">

			{% for s in all_students %}
			{% if (s.key.id() == id) %}
			
			<fieldset>
			<legend><h1 style="color: #FFFFFF;"><font style="Segoe UI">Add New Student &nbsp&raquo;</font></h1></legend>
			</fieldset>

				<form method = "POST" action ="/student/new"> 
					

					<!-- Names -->
					<div class="input-group col-md-90">
					<table>
					<tr>
						<td>
						<div class="input-group col-md-30">
							<span class="input-group-addon labelbg">Name :&nbsp;&nbsp;&nbsp;</span>
							<input type = "text" class="form-control" class="form-control" pattern="^[a-zA-z -(50)]*" name= "first_name" placeholder="First name" value="{{s.first_name}}" required>
						</div>
						</td>

						<td>
						<div class="input-group col-md-30">
							<input type = "text" class="form-control" class="form-control" pattern="^[a-zA-z -(20)]*" name="middle_name" placeholder="Middle name" value="{{s.middle_name}}">
						</div>
						</td>

						<td>
						<div class="input-group col-md-30">
							<input type = "text" class="form-control" class="form-control" pattern="^[a-zA-z -(20)]*" name= "last_name" placeholder="Last name" value="{{s.last_name}}" required>
						</div>
						</td>
					</tr>
					</table>
					</div>

					<br/>




					<!-- Email -->
					<div class="input-group col-md-8">
						<span class="input-group-addon labelbg">Email :&nbsp;&nbsp;&nbsp;&nbsp;</span>
							<input type = "text" class="form-control" name= "email" class="form-control" name= "email" placeholder="email@net.com" value="{{s.email}}" required>
    				</div>
					
					<br>
					

					<!-- student number -->

					<div class="input-group col-md-8">
							<span class="input-group-addon labelbg">Student Number :</span>
							<input type = "text" class="form-control" name= "stud_num" class="form-control" value="{{s.stud_num}}" required>

					</div>
					<br>




					<!-- phone number -->
					<div class="input-group col-md-8">
							<span class="input-group-addon labelbg">Contact Number :</span>
							<input type = "text" class="form-control" name= "numb" class="form-control" pattern="^[0-9(20)]*" value="{{s.numb}}" required>

					</div>
					<br>



					<!--DEPT -->

					<div class="input-group col-md-9">
						<span class="input-group-addon">College : &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>
							<select name="college" class="form-control">
							  <option> {{s.college}} </option>
							  <option value="Computer Engineering"> Computer Engineering </option>
							  <option value="Electronics and Communication Egineering"> Electronics and Communication Egineering </option>
							  <option value="Electrical Ebgineering"> Electrical Engineering</option>
							  <option value="Mechanical Engineering"> Mechanical Engineering </option>	
							  <option value="Civil Engineering"> Civil Engineering </option>
							  <option value="Industrial Engineering"> Industrial Engineering </option>
							</select>
					</div>

					<hr>
					<!-- Submit Button -->
					<input type = "Submit" class="btn pull-right" id="studbtn">

					{% endif %}
					{% endfor %}

				</form>
				</fieldset>
			</div>
			</div>
		</div>
	</div>


	<!-- Footer -->
	<div id="footer-style" class="footer-style navbar navbar-default navbar-fixed-bottom";>
		<div class="container">
			<p class="navbar-text text-right" style="font: 10pt Tahoma; color: #B7154D;"> Site built by Team 9 :) </p>
		</div>
	</div>






{% else %}

<!-- Header -->
  <div id="header" class="navbar navbar-inverse navbar-static-top " >
  
    <div id="header_container" class="container">
      <a href="/" class = "navbar-brand"><h3 style="font-style: Calibri; color:#E6E6E6;">Module 3<h3></a>

      <div>
            &nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp<ul class="nav nav-pills">
          <li class="#"><a href="/">Guestbook</a></li>
          <li class="dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown"> Profiles <span class="caret"></span></a>
          <ul class="dropdown-menu">
              <li><a href="/module-1/1"> Profile One</a></li>
                      <li><a href="/module-1/2"> Profile Two</a></li>
            </ul>       
          </li>
          <li><a href="/student/list">Students</a></li>
          <li><a href="/thesis/list">Thesis</a></li>
          <li><a href="/adviser/list">Advisers</a></li>
      </ul>
      </div>


    </div>  

  </div>

<!-- Header End -->


<div class="col-md-20" id="loginstudent">
    <p align="center">
    <br><br><br><br>
    <font style="font: 30pt Calibri; color: #CBCBCB; ">
    Please <a color= #FE8196 href="{{ url|safe }}"> {{ url_linktext }}</a><br> before entering the page.
    </font>
    </p>
</div>
</body>



  <!-- Footer -->
  <div id="footer-style" class="footer-style navbar navbar-default navbar-fixed-bottom";>
    <div class="container">
      <p class="navbar-text text-right" style="font: 10pt Tahoma; color: #B7154D;"> Site built by Team 9 :) </p>
    </div>
  </div>

{% endif %}







<!-- Scripts -->
	<script src = "http://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
	<script src= "js/bootstrap.js"></script>
    <script src="/js/jquery.js"></script>
    <script src="/js/bootstrap-transition.js"></script>
    <script src="/js/bootstrap-alert.js"></script>
    <script src="/js/bootstrap-modal.js"></script>
    <script src="/js/bootstrap-dropdown.js"></script>
    <script src="/js/bootstrap-scrollspy.js"></script>
    <script src="/js/bootstrap-tab.js"></script>	
    <script src="/js/bootstrap-tooltip.js"></script>
    <script src="/js/bootstrap-popover.js"></script>
    <script src="/js/bootstrap-button.js"></script>
    <script src="/js/bootstrap-collapse.js"></script>
    <script src="/js/bootstrap-carousel.js"></script>
    <script src="/js/bootstrap-typeahead.js"></script>


<!--end script -->

</body>
</html>